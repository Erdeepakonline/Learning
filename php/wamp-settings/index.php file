if(is_dir($file) && !in_array($file,$projectsListIgnore)){
		$projectContents .= ($wampConf['LinksOnProjectsHomePage'] == 'on') ? "<li><a href='http://localhost/".$file."/'>".$file."</a></li>" : '<li><a href="'.$file.'/">'.$file.'</a></li>';
	}
